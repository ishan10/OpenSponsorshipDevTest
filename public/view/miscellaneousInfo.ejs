<div class="center">
    <div class="textCenter"><h5>Miscellaneous Information</h5></div>
    <div class="form-group">
        <label for="pets">Do you have pets?</label>
        <input type="checkbox" id="pets" value="pets" ng-model="profile.pets">
    </div>
    <div class="form-group">
        <label for="alcohol">Do you drink alcohol?</label>
        <input type="checkbox" id="alcohol" value="alcohol" ng-model="profile.alcohol">
    </div>
    <div class="form-group">
        <label for="fb">Let us know if you are interested in any charities.</label>
        <input type="text" class="form-control" id="fb" placeholder="Enter charities" ng-model="profile.charity">
    </div>
    <div class="float-right">
        <a ui-sref="profileForm.review">
            <button type="button" class="btn btn-primary">Review</button>
        </a>
    </div>
</div>